---
import SiteLayout from "@layouts/SiteLayout.astro";
import { createClient } from "@supabase/supabase-js";
const supabase = createClient( import.meta.env.SUPABASE_URL, import.meta.env.SUPABASE_KEY );
const { data, error } = await supabase.from("likes").select("category, name");
---
<SiteLayout>
  <p>Ongoing record of the latest games, films, and albums I've enjoyed</p>
  {
    data?.map((item) => (
      <>
        <h2>{item.category}</h2>
        <ul>{item.name}</ul>
      </>
    ))
  }
</SiteLayout>

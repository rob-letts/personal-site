{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "moduleResolution": "node",
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "alwaysStrict": true,
    "allowUnreachableCode": false,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "types": ["vite/client"]
    // "rootDir": "./",
    // "baseUrl": "./",
    // "types": [],
    // "allowSyntheticDefaultImports": true,
  },
  "exclude": [
    "node_modules/**/*"
  ]
}